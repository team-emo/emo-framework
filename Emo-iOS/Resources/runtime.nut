        emo.Event().addOnUpdateListener(this);
        
        padding = 0;
        margin  = 0;

        updateInterval = 100;
        lastUpdate = -1000;
    }
    
    function load() {
        base.load();
        knob.load();
    }
    
    function show() { knob.show(); return base.show(); }
    function hide() { knob.hide(); return knobase.hide(); }
