    function red  (r = null) { knob.red(r);   return base.red(r);   }
    function green(g = null) { knob.green(g); return base.green(g); }
    function blue (b = null) { knob.blue(b);  return base.blue(b);  }

    function move(x, y, z = 99) {
        releaseKnob(x, y, z + 1);
        return base.move(x, y, z);
    }
    
